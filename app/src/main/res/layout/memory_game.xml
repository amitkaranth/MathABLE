<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#202124"
    tools:context=".welcome_page">

    <ImageButton
        android:id="@+id/ib5"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginTop="200dp"
        android:layout_marginRight="40dp"
        android:background="#202124"
        android:backgroundTint="#202124"
        android:scaleType="fitCenter"
        android:tooltipText="addition"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/qmark"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,UnusedAttribute" />

    <ImageButton
        android:id="@+id/ib6"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginTop="300dp"
        android:layout_marginRight="40dp"
        android:scaleType="fitCenter"
        android:background="#202124"
        android:backgroundTint="#202124"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/qmark"
        tools:ignore="ContentDescription,RtlHardcoded,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ib8"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginTop="500dp"
        android:layout_marginEnd="40dp"
        android:background="#202124"
        android:backgroundTint="#202124"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/qmark"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ib7"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginTop="400dp"
        android:layout_marginRight="40dp"
        android:background="#202124"
        android:backgroundTint="#202124"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/qmark"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="117dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="117dp"
        android:text="Match the following"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ImageButton
        android:id="@+id/ib1"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="200dp"
        android:background="#202124"
        android:backgroundTint="#202124"
        android:scaleType="fitCenter"
        android:tooltipText="add"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/qmark"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ib2"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="300dp"
        android:background="#202124"
        android:backgroundTint="#202124"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/qmark"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ib3"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="400dp"
        android:background="#202124"
        android:backgroundTint="#202124"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/qmark"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/ib4"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="500dp"
        android:background="#202124"
        android:backgroundTint="#202124"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/qmark"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="165dp"
        android:layout_marginTop="159dp"
        android:layout_marginEnd="168dp"
        android:text="Points: 0"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>